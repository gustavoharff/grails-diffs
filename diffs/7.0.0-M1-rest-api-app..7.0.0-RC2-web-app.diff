+                        <li>Exception: ${exception}</li>
+                        <li>Message: ${message}</li>
+                        <li>Path: ${path}</li>
+                    </ul>
+                </g:else>
+            </g:if>
+            <g:else>
+                <ul class="errors">
+                    <li>An error has occurred</li>
+                </ul>
+            </g:else>
